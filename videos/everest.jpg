         }
         if (subscriptionPlanStatus === "" && adFree == true) {
             ga('set', 'dimension16', 'Subscribed');
             var apiUrl = '/subscription/fetch/userinfo/' + token;
             var xhttp = new XMLHttpRequest;
             xhttp.responseType = "json";
             xhttp.open("GET", apiUrl, true);
             xhttp.send();
             xhttp.onreadystatechange = function () {
                 if (xhttp.readyState == 4 && xhttp.status == 200) {
                     var data = xhttp.response;
                     var subscriberInfo = data.plan;
                     var subscription_user_status;
                     if (subscriberInfo != null && subscriberInfo != "undefined") {
                         if (subscriberInfo.subscriptions.length) {
                             subscriptionPlanStatus = subscriberInfo.subscriptions[0].status;
                             if(subscriptionPlanStatus == "live" || subscriptionPlanStatus == "non_renewing" || subscriptionPlanStatus == "dunning" || subscriptionPlanStatus == "unpaid"){
                                subscription_user_status = "live_full";
                             }else if(subscriptionPlanStatus == "trial"){
                                subscription_user_status = "live_trial";
                             }else if(subscriptionPlanStatus == "cancelled" || subscriptionPlanStatus == "expired" || subscriptionPlanStatus == "cancelled_from_dunning" || subscriptionPlanStatus == "deleted" || subscriptionPlanStatus == "paused"){
                                 subscription_user_status = "churned";
                             }
                             subscriptionStartDate = subscriberInfo.subscriptions[0].activated_at;
                             subscription_number = subscriberInfo.subscriptions[0].subscription_number;

                             customFields = subscriberInfo.subscriptions[0].custom_fields;
                             if (customFields !== null || customFields !== undefined) {
                                 customFields.forEach(function (item) {
                                     Object.keys(item).forEach(function (key) {
                                         if (item.label === 'partner') {
                                             partnerValue = item.value;
                                             return;
                                         }
                                     });
                                 });
                             }
                             if (clientId !== "undefined" && clientId) {
                                         ga('set', 'dimension8', clientId);
                                         dapTracker.track("subscriber_info", {
                                             "subscription_number":subscription_number,
                                             "subscription_user_status": subscription_user_status,
                                             "subscription_start_date": subscriptionStartDate,
                                             "partner": partnerValue,
                                             "product": "LM",
                                             "platform": platform
                                         }, "event");
                                     }
                         }
                     }
                 }
             }
         }

         if (adFree == false && token) {
             ga('set', 'dimension16', 'NonSubscribed_loggedin');
         } else if (adFree == false && token == null) {
             ga('set', 'dimension16', 'NonSubscribed_nonloggedin');
         }

     }
</script>
