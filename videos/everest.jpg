        return (
            distance.top >= 0 &&
            distance.left >= 0 &&
            ((distance.bottom - bottomDistance) <= (window.innerHeight || document.documentElement.clientHeight)) &&
            Math.round(distance.right) <= (window.innerWidth || document.documentElement.clientWidth)
        );
    }
